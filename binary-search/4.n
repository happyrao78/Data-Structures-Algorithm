// search insert position 
we have to return that index jha pe wo value insert ho ske so bacially lower bound question

int insrtPos(vector<int> &arr, int n, int x){
    int low=0;
    int high=n-1;
    int ans=-1;
    while(low<=high){
        int mid=(low+high)/2;
        if(arr[mid]>=x){
            ans=mid;
            high=mid-1;
        }
        else{
            low=mid+1;
        }
    }
    return ans;
}
